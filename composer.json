{
    "name": "hautelook/alice-bundle",
    "description": "Symfony2 Bundle that helps loading fixture data written with the Alice library",
    "keywords": ["Symfony2", "Fixture", "ORM", "Alice"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Baldur Rensch",
            "email": "brensch@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "nelmio/alice": "dev-bugfix",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "doctrine/doctrine-bundle": "~1.2"
    },
    "require-dev": {
        "symfony/framework-bundle": "~2.1",
        "symfony/console": "~2.1",
        "symfony/yaml": "~2.1",
        "doctrine/orm": "~2.4",
        "symfony/browser-kit": "~2.1"
    },
    "autoload": {
        "psr-0": { "Hautelook\\AliceBundle": "" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nVitius/alice.git"
        }
    ],
    "target-dir": "Hautelook/AliceBundle"
}
